{
    "name": "otisz/laravel-eloquent-filter",
    "description": "Flexible Eloquent filter from URL queries",
    "keywords": [
        "laravel",
        "eloquent",
        "laravel-query-builder",
        "laravel-eloquent-builder",
        "query",
        "queries",
        "builder",
        "filter",
        "search",
        "order",
        "sort"
    ],
    "homepage": "https://github.com/Otisz/laravel-eloquent-filter",
    "license": "MIT",
    "authors": [
      {
        "name": "Levente Otta",
        "email": "leventeotta@gmail.com",
        "homepage": "https://leventeotta.com"
      }
    ],
    "type": "library",
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.7.0|~5.8.0|^6.0",
        "illuminate/http": "~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0"
    },
    "autoload": {
      "psr-4": {
        "Otisz\\EloquentFilter\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Otisz\\EloquentFilter\\Tests\\": "tests"
      }
    },
    "scripts": {
      "test": "vendor/bin/phpunit"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Otisz\\EloquentFilter\\EloquentFilterServiceProvider"
        ]
      }
    },
    "minimum-stability": "stable"
}
