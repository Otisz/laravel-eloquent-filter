{
    "name": "otisz/laravel-eloquent-filter",
    "description": "Flexible Eloquent filter from URL queries",
    "keywords": [
        "laravel",
        "eloquent",
        "laravel-query-builder",
        "laravel-eloquent-builder",
        "query",
        "queries",
        "builder",
        "filter",
        "search",
        "order",
        "sort"
    ],
    "homepage": "https://github.com/Otisz/laravel-eloquent-filter",
    "license": "MIT",
    "authors": [
      {
        "name": "Levente Otta",
        "email": "leventeotta@gmail.com",
        "homepage": "https://leventeotta.com"
      }
    ],
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.12|^9.0|^10.0|^11.0",
        "illuminate/http": "^8.12|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.12|^9.0|^10.0|^11.0"
    },
    "autoload": {
      "psr-4": {
        "Otisz\\EloquentFilter\\": "src"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Otisz\\EloquentFilter\\EloquentFilterServiceProvider"
        ]
      }
    },
    "minimum-stability": "stable"
}
